{{ config(materialized='table') }}

SELECT * FROM IEDM1DB.PROJECT.orders;


CREATE OR REPLACE VIEW 
        orders_train AS 
        SELECT * FROM IEDM1DB.PROJECT.orders
        WHERE eval_set = 'train';
        
CREATE OR REPLACE VIEW 
        orders_test AS 
        SELECT * FROM IEDM1DB.PROJECT.orders
        WHERE eval_set = 'test';